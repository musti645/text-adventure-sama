{
    "version": "0.2.1",
    "configurations": [
      {
        "name": "Launch Chrome",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:4200/#",
        "webRoot": "${workspaceFolder}"
      },
      {
        "name": "Attach Chrome",
        "type": "chrome",
        "request": "attach",
        "url": "http://localhost:4200/#",
        "webRoot": "${workspaceFolder}"
      },
      {
        "name": "Launch Chrome (Test)",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:9876/debug.html",
        "webRoot": "${workspaceFolder}/text-adventure-sama"
      },
      {
        "name": "Launch Chrome (E2E)",
        "type": "node",
        "request": "launch",
        "program": "${workspaceFolder}/text-adventure-sama/node_modules/protractor/bin/protractor",
        "protocol": "inspector",
        "args": ["${workspaceFolder}/protractor.conf.js"]
      }
    ]
  }